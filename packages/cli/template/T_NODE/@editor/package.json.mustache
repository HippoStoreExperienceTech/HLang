    "@vitejs/plugin-react": "4.x",
    "babel-eslint": "^10.1.0",
    "enzyme": "3.x",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "7.x",
    "npm-ensure": "^1.2.0",
    "react": "^18.x",
    "react-dom": "^18.x",
    "sass-embedded": "^1.77.8",
