    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run eslint && npm-ensure -t deps",
    "eslint": "eslint '../@editor/**/*.@(js|jsx)' --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "../@editor/**/*.@(js|jsx)": [
      "eslint --fix",
      "npm-ensure -t deps",
      "git add"
    ]
  },
  "ensure": {
    "deps": {
      "checkDirs": [
        "../@editor/**/*"
      ],
      "ignores": [
        "components",
        "utils",
        "widget",
        "styles"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
