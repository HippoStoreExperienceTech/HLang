{
  "name": "@hset/hlang-runtime",
  "version": "0.0.1-BETA",
  "description": "hlang runime",
  "main": "lib/index.js",
  "type": "lib/index.d.ts",
  "scripts": {
    "test": "tnpm run build && jest --coverage --forceExit",
    "build": "tsc -p tsconfig.json",
    "lint": "npm run eslint && npm-ensure -t deps",
    "eslint": "eslint 'src/**/*.@(js|jsx)' --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.@(js|jsx)": [
      "eslint --fix",
      "npm-ensure -t deps",
      "git add"
    ]
  },
  "ensure": {
    "deps": {
      "checkDirs": [
        "src/**/*"
      ],
      "ignores": [
        "components",
        "utils",
        "widget",
        "styles"
      ]
    }
  },
  "author": "xiaoa",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "moment": "^2.29.1",
    "rxjs": "^6.6.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ali/eslint-config-wdk": "^1.0.25",
    "@types/jest": "^27.4.0",
    "babel-eslint": "^10.1.0",
    "enzyme": "3.x",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^27.5.1",
    "lint-staged": "7.x",
    "moment": "^2.29.1",
    "npm-ensure": "^1.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
